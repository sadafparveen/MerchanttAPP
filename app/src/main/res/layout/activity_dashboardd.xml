<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:context=".MainActivity"
        tools:openDrawer="start">


    <RelativeLayout android:layout_width="match_parent"
                    android:layout_height="70dp"

>
        <ImageView
                android:layout_width="match_parent"
                android:layout_height="285dp"
                app:srcCompat="@drawable/yu"
                android:id="@+id/imageView30"
                />
        <androidx.appcompat.widget.Toolbar
                android:id="@+id/actionbar"
                android:background="@drawable/border"

                android:layout_width="match_parent"
                android:layout_height="70dp"
                app:title="Dashboard"
                app:titleMarginStart="90dp"
                app:titleTextColor="#070505">


        </androidx.appcompat.widget.Toolbar>


        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_marginLeft="20dp" android:layout_marginRight="20dp"
                android:layout_marginTop="80dp" android:layout_marginBottom="30dp"
                android:background="@drawable/rounded_layout"
                android:elevation="3dp">

            <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="80dp" android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginTop="30dp">
                <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        app:srcCompat="@drawable/ic_baseline_list_alt_24"
                        android:id="@+id/imageView7" android:layout_marginTop="20dp" android:layout_marginLeft="20dp"
                        tools:ignore="VectorDrawableCompat"/>
                <TextView
                        android:text="Incoming Orders"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#8C7E7E"
                        android:id="@+id/textView"
                        android:layout_marginLeft="80dp"
                        android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:fontFamily="@font/anaheim" tools:ignore="DuplicateIds" android:textSize="18sp"/>
                <ImageView
                        android:layout_width="36dp"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                        android:id="@+id/imageView25" android:layout_marginTop="25dp" android:layout_marginLeft="220dp"
                        tools:ignore="VectorDrawableCompat"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="80dp" android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginTop="230dp">
                <ImageView
                        android:layout_width="36dp"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                        android:id="@+id/imageView26" android:layout_marginTop="25dp" android:layout_marginLeft="220dp"
                        tools:ignore="VectorDrawableCompat"/>
                <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        app:srcCompat="@drawable/ic_baseline_airport_shuttle_24"
                        android:id="@+id/imageView7"
                        android:layout_marginTop="20dp" android:layout_marginLeft="20dp"
                        tools:ignore="DuplicateIds,VectorDrawableCompat"/>
                <TextView
                        android:text="Delivered Orders"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#8C7E7E"
                        android:id="@+id/textView" android:layout_marginLeft="83dp"
                        android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:fontFamily="@font/anaheim" tools:ignore="DuplicateIds" android:textSize="18sp"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                    android:id="@+id/cardview1"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_marginTop="130dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp">
                <ImageView
                        android:layout_width="36dp"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                        android:id="@+id/imageView27" android:layout_marginTop="25dp" android:layout_marginLeft="220dp"
                        tools:ignore="VectorDrawableCompat"/>
                <TextView
                        android:text="Product"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#8C7E7E"
                        android:id="@+id/textView2" android:layout_marginLeft="90dp"
                        android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:fontFamily="@font/anaheim" android:textSize="18sp"/>
                <ImageView
                        android:layout_width="40dp"
                        android:layout_height="43dp" app:srcCompat="@drawable/pro" android:id="@+id/imageView9"
                        android:layout_marginTop="18dp" android:layout_marginLeft="20dp"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    app:cardCornerRadius="10dp"
                    android:layout_height="80dp" android:layout_marginTop="330dp" android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp">
                <ImageView
                        android:layout_width="36dp"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ic_baseline_arrow_forward_ios_24"
                        android:id="@+id/imageView28" android:layout_marginTop="25dp" android:layout_marginLeft="220dp"
                        tools:ignore="VectorDrawableCompat"/>
                <TextView
                        android:text="Reports"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#8C7E7E"
                        android:id="@+id/textView4" android:layout_marginLeft="90dp"
                        android:layout_marginTop="25dp" android:textAppearance="@style/TextAppearance.AppCompat.Large"
                        android:fontFamily="@font/anaheim" android:textSize="18sp"/>
                <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        app:srcCompat="@drawable/ic_baseline_signal_cellular_alt_24"
                        android:id="@+id/imageView6" tools:ignore="VectorDrawableCompat" android:layout_marginTop="20dp"
                        android:layout_marginLeft="20dp"/>
            </androidx.cardview.widget.CardView>
        </RelativeLayout>

    </RelativeLayout>






    <com.google.android.material.navigation.NavigationView
            android:layout_gravity="start"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:id="@+id/nav_bar"
            app:headerLayout="@layout/header"
            app:menu="@menu/main_menu"/>




</androidx.drawerlayout.widget.DrawerLayout>